  <script type="text/javascript" src="http://mottie.github.io/tablesorter/dist/js/jquery.tablesorter.min.js"></script>
  <script type="text/javascript" src="http://mottie.github.io/tablesorter/dist/js/jquery.tablesorter.widgets.min.js"></script>
  <%= csrf_meta_tags %>
